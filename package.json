{
  "name": "@spool/cms",
  "version": "0.1.0",
  "description": "Beautiful headless CMS for Next.js with real-time editing and AI-native content",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://spool.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/spool"
  },
  "keywords": [
    "cms",
    "headless",
    "nextjs",
    "content-management",
    "typescript",
    "supabase",
    "real-time"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev & (sleep 10 && node scripts/precompile-routes.js)",
    "precompile": "node scripts/precompile-routes.js",
    "build": "next build && node esbuild.config.js && node process-css.js",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:stripe": "node scripts/test-stripe-integration.js",
    "type-check": "tsc --noEmit",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:smoke": "playwright test tests/smoke.spec.ts",
    "db:generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/lib/supabase/database.types.ts",
    "db:reset": "supabase db reset",
    "db:seed": "node scripts/seed-database.js",
    "migrate": "supabase migration new"
  },
